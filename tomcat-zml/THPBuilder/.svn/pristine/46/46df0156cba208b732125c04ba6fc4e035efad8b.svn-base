.setting-box button,.edit-box button {width:165px;height:45px;background:#00a0e9;color:#fff;font-size: 18px;font-weight: bold;border:none;border-radius: 3px;margin-left: 32px;margin-bottom: 10px;}
.setting-box form {margin-left: 300px;}

