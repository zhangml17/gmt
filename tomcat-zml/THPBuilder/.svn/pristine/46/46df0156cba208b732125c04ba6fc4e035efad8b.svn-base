.banner-video { position: absolute; top:150px; right:15px; width:500px;  }
.youshi-container { background-color: #fff; padding-top: 20px; -webkit-box-shadow:0 0 3px rgba(0, 0, 0, .3); -moz-box-shadow:0 0 3px rgba(0, 0, 0, .3);  box-shadow:0 0 3px rgba(0, 0, 0, .3);}
.thumbnail { border:none; padding-top: 12px;}
.thumbnail h3 { text-align: center; margin-top: 14px;}
.thumbnail p { max-height: 120px;line-height: 2;}
.jingdian {padding-top: 30px;}
.jingdian h4 { text-align: center;}
.jingdian img { width:100%; }
.col-md-5 .jingdian-box { height:275px;}
.col-md-7 .jingdian-box { height:122px; }
.col-md-6 img { height:122px;}
.col-txt { height:109px;}
.row-txt { height:122px; }
.jingdian-box { cursor:pointer; background-color: #fff; -webkit-box-shadow:0 0 3px rgba(0, 0, 0, .3);  -moz-box-shadow:0 0 3px rgba(0, 0, 0, .3);  box-shadow:0 0 3px rgba(0, 0, 0, .3); transition:.3s;}
.jingdian-txt {-moz-transition:.4s; -webkit-transition:.4s; transition:.4s;}
.jingdian-box:hover .jingdian-txt { background: #00a0e9; color:#fff; }
.jingdian-txt h4 { padding-top: 10px; margin-top: 0;}
.jingdian-right dl { background-color:#fff; -webkit-box-shadow:0 0 3px rgba(0, 0, 0, .3);  -moz-box-shadow:0 0 3px rgba(0, 0, 0, .3);  box-shadow:0 0 3px rgba(0, 0, 0, .3);}
.jingdian-txt p {padding:0 15px 10px; margin-bottom: 0;}
.jingdian-right dt { padding-left:15px; height:52px; line-height: 53px; font-size: 16px; font-weight: bold; border-bottom:1px solid #ccc;}
.jingdian-right dt a { float: right; line-height: 52px; font-size: 14px; color:#666; font-weight: normal; margin-right: 15px;}
.jingdian-right dt a:hover { color:#00a0e9;}
.jingdian-right dd a { display: block; color: #333; height:44px; line-height: 45px; padding-left:15px; background:url(../images/dianlu-icon.png) no-repeat 15px center; -moz-transition:.3s; -webkit-transition:.3s; transition:.3s;}
.jingdian-right dd span {display:inline-block; width:45px;margin-left: 26px; margin-right: 10px; color:#fff; }
.jingdian-right dd a:hover { color:#00a0e9; background:url(../images/dianlu-icon-hover.png) no-repeat 15px center; }
.footer { background: #ddd; padding:20px 0 10px;color:#000;bottom: 0;left:0;width:100%;}
.footer p { text-align: center; color:#000000;font-size: 17px;}
.footer p a { color:#000000; margin:0 8px;font-size: 17px;}
.test-button{border: none;text-align:center;width:320px;top:470px;font-size: 30px;border-radius: 5px;height: 60px;position: absolute;background-color:#00a0e9;color: #fff;font-weight: bold;}

/*我的资料*/
.user-setting { position: relative; top:100px; background-color: #fff; border-radius: 5px; -webkit-box-shadow:0 0 5px rgba(0, 0, 0, .3); -moz-box-shadow:0 0 5px rgba(0, 0, 0, .3);  box-shadow:0 0 5px rgba(0, 0, 0, .3); }
.avatar {position: absolute;text-align: center;left: 50px;top:45px;}
.avatar img { margin-bottom: 10px;}
.avatar a { text-decoration: underline; }
.setting-box {width: 80%;float: left;margin: 8px 5px 5px 50px;border: 1px solid #fff;background-color: #fff;box-shadow: 0 1px 2px rgba(0,0,0,0.2);box-shadow: none\9;padding: 0;position: relative;overflow: visible;/* margin-right: 50px; *//* margin-left: 348px; */}
