/*fz*/
.navbar { margin-bottom:0; border-radius: 0; background-color: #fff; box-shadow:0 2px 3px rgba(0, 0, 0, .3);position: fixed;width: 100%;z-index: 21;    border-bottom: 2px solid #00a0e9;}
.fz-container {    margin-bottom:0; }
.navbar-logo { display: block; width:480px; height:70px;  line-height: 65px; background: url(../images/header-logo.png) no-repeat 15px center; text-indent: -9999px;}
.fz-navbar-nav { margin-left: 50px;}
.fz-navbar-nav>li>a { line-height:40px; font-size: 16px;font-weight: bold; }
.navbar-nav>.active>a { font-weight: bold; color:#00a0e9;}
.nav>li>a:focus, .nav>li>a:hover, .nav>li>a.active { background-color:#fff; color:#00a0e9;}
.btn.active.focus, .btn.active:focus, .btn.focus, .btn:active.focus, .btn:active:focus, .btn:focus { outline:none;}
/*.form-control:focus { border-color:#703a99; box-shadow: inset 0 1px 1px rgba(0,0,0,.075),0 0 8px rgba(155,69,204,.6);}*/
.search-form { position: relative; margin-top:20px; }
.search-ipt { width:170px; height:30px; padding-right: 24px; line-height: 30px; border-radius: 0; outline: none;}
.search-btn { position: absolute; left:180px; top:6px; width:18px; height:18px;  background: url(../images/search-btn.png) no-repeat center; text-indent:-9999px; border:none;}
.search-btn:hover,.search-btn.active { background: url(../images/search-btn-hover.png) no-repeat center;}
.personal-box img { margin-left: 20px; margin-right:5px;}
.nav .open>a, .nav .open>a:focus, .nav .open>a:hover { background-color: #fff;}
.dropdown-menu { background-color: #00a0e9; z-index: 100000; }
.dropdown-menu>li>a { color:#fff; line-height: 40px; padding-left:40px;}
.dropdown-menu>li>a.my-work { background: url(../images/my-work.png) no-repeat 15px center;}
.dropdown-menu>li>a.my-item { background: url(../images/my-item.png) no-repeat 15px center;}
.dropdown-menu>li>a.my-detail { background: url(../images/my-detail.png) no-repeat 15px center;}
.dropdown-menu>li>a.my-quit { background: url(../images/my-quit.png) no-repeat 15px center;}
.dropdown-menu>li>a:focus, .dropdown-menu>li>a:hover { color:#fff; background-color:#0292d4; }

#fz-banner { height:680px; margin-top:-1px; margin-bottom: 0px;}
.banner-container { position:relative; height: 540px; }
.banner-container h2, .banner-container h3, .banner-container ul  { position: absolute; padding:0 20px;     color:#fff; background:rgba(0, 0, 0, .4); }
.banner-container h2 { top:150px; line-height: 60px; font-size: 34px; font-weight: bold; }
