/*my-project*/
.my-project { padding:103px 0;}
.sidebar  { background: #fff; padding-bottom: 30px; }
.sidebar h4, .project-box h4 { padding:0 15px; line-height: 45px; margin-top: 0; margin-bottom: 0; border-bottom: 1px solid #ccc; font-weight: bold; }
.sidebar h4 a { padding:0 8px; float: right; font-size: 20px; font-weight: bold;}
.sidebar .menu { padding:0 15px; }
.sidebar .menu a { font-size: 16px; line-height: 50px; padding-left: 65px; display: block; border-bottom: 1px solid #ccc; -moz-transition:.3s; -webkit-transition:.3s; transition:.3s;}
.sidebar .menu a:focus { text-decoration: none;}
.sidebar .menu a.active { color:#00a0e9; font-weight: bold;}
.sidebar .menu a.item-all { background:url(../images/item-all.png) no-repeat 40px center;}
.sidebar .menu a.item-all:hover, .sidebar .menu a.item-all.active { background:url(../images/item-all-act.png) no-repeat 40px center; }
.sidebar .menu a.item-create { background:url(../images/item-create.png) no-repeat 40px center;}
.sidebar .menu a.item-create:hover, .sidebar .menu a.item-create.active { background:url(../images/item-create-act.png) no-repeat 40px center;}
.sidebar .menu a.item-file { background:url(../images/item-file.png) no-repeat 40px center;}
.sidebar .menu a.item-file:hover, .sidebar .menu a.item-file.active { background:url(../images/item-file-act.png) no-repeat 40px center;}
.sidebar .menu a.item-atten { background:url(../images/item-atten.png) no-repeat 40px center;}
.sidebar .menu a.item-atten:hover, .sidebar .menu a.item-atten.active { background:url(../images/item-atten-act.png) no-repeat 40px center;}
.sidebar .menu a.item-collect { background:url(../images/item-collect.png) no-repeat 40px center;}
.sidebar .menu a.item-collect:hover, .sidebar .menu a.item-collect.active { background:url(../images/item-collect-act.png) no-repeat 40px center;}
.sidebar .menu a.item-markets { background:url(../images/item-market.png) no-repeat 40px center;}
.sidebar .menu a.item-markets:hover, .sidebar .menu a.item-markets.active { background:url(../images/item-market-act.png) no-repeat 40px center;}
.sidebar-dropdown { display: none;}

.project-box { background-color: #fff;  }
.project-box .img-box img { width:100%; }
.project-box .item { padding:15px 0; border-bottom: 1px solid #ccc;}
.item .img-box { padding-left: 15px;}
.txt-box h3 { margin-top: 0; font-size: 18px; margin-bottom: 5px; font-weight: bold;}
.txt-box p { margin-bottom: 0; line-height: 22px;}
.txt-box p span { color:#666;}
.txt-box p span.info { color:black;}
.txt-box a { display: inline-block; background: #00a0e9; color:#fff; margin-right: 10px; }
.txt-box a:hover, .txt-box a:visited { color:#fff;}
