/*cuowu*/
.fullwidth{margin:0;text-align:center}
.fullwidth.white{background-color:#fff}
.fullwidth.gray{background-color:#f9f9f9}
html[lang=zh-cn] .fullwidth .ui.button{letter-spacing:.15em}
html[lang=zh-cn] .fullwidth .ui.button,html[lang=zh-cn] .fullwidth h1,html[lang=zh-cn] .fullwidth h2,html[lang=zh-cn] .fullwidth h3{font-weight:400}
.fullwidth h1{font-size:6.86rem;padding:0;margin:0;color:#454545}
.fullwidth h3{font-size:1.8rem;padding:0;margin:0;color:#887;line-height:1.8}
.fullwidth.section-1{margin:-.85rem 0 0;padding:66px 0 72px;background:#8bbfe5 url(images/v3/home-banner-bg.png) no-repeat center center;box-shadow:0 -1px 2px 0 #999,0 1px 2px 0 #999}
.fullwidth.section-1 h1{margin:0;color:#fff}
.fullwidth.section-1 h2{margin-top:36px;color:#d7eaf9}
.fullwidth.section-1 h2 strong{color:#fff}
.fullwidth.section-1 .laptop{margin:48px auto 0;width:548px;height:291px;overflow:hidden;position:relative;background-image:url(images/v3/homepage-laptop.png)}
.fullwidth.section-1 .laptop .play-gray,.fullwidth.section-1 .laptop .play-orange{display:block;position:absolute;left:74px;top:18px;width:401px;height:237px;text-indent:-9999px;text-decoration:none;background-position:-74px -18px}
.fullwidth.section-1 .laptop .play-gray{background-image:url(images/v3/homepage-laptop-play-gray.png)}
.fullwidth.section-1 .laptop .play-orange{background-image:url(images/v3/homepage-laptop-play-orange.png)}
.fullwidth.section-1 .laptop:hover .play-gray{top:-999px}
.fullwidth.section-1 .laptop:not(:hover) .play-orange{top:-999px}
.fullwidth.section-2{padding:140px 0 130px}
.fullwidth.section-2>h3{margin:30px auto 35px;max-width:1280px}
.fullwidth .two-columns{margin:60px auto 85px;font-size:0;max-width:1280px}
.fullwidth .two-columns>div{display:inline-block;vertical-align:top;width:100%;font-size:3rem;text-align:left}
.fullwidth .two-columns>div.image{padding-top:0px}
.fullwidth .two-columns>div.image>img{display:block;border:1px solid rgba(0,0,0,.3);border-radius:3px;width:100%;height:385px;box-shadow:0 0 6px 0 #bbb}
.fullwidth .two-columns div+.list{padding-left:40px;box-sizing:border-box}
.fullwidth .two-columns .list:first-child{padding-right:40px;box-sizing:border-box}
.fullwidth .two-columns .item{position:relative;padding-left:88px}
.fullwidth .two-columns .icon{position:absolute;display:block;width:48px;height:48px;left:0;top:10px}
.fullwidth .two-columns .item h2{margin:0 0 7px}
.fullwidth .two-columns .item h3{margin:0}
.fullwidth .two-columns .item:not(:last-child){margin-bottom:40px}
.fullwidth.section-3{padding:100px 0 130px}
.fullwidth.section-3>h3{margin:30px auto 35px;max-width:1280px}
.fullwidth.section-4{padding:140px 0 130px}font-weight: 400;
.fullwidth.section-4>h3{margin:30px auto 35px;max-width:1280px}
.fullwidth.section-5{padding:140px 0 130px}
.fullwidth.section-6{padding:140px 0 130px}
.fullwidth.section-6 h2{color:#887}
.fullwidth.section-6>h3{margin:30px auto 35px;max-width:1280px}
.fullwidth.section-6>h3 strong{color:orange}
.fullwidth.section-6 .gallery{max-width:1140px;margin:45px auto 20px;border:solid 0 #d3d3d3;position:relative}
.fullwidth.section-6 .gallery *{vertical-align:top}
.fullwidth.section-6 .gallery>.icon{font-size:2rem;color:#8BBFE5;position:absolute;top:50%;margin-top:-2rem;cursor:pointer}
.fullwidth.section-6 .gallery>.icon:hover{color:#78b4e0}
.fullwidth.section-6 .gallery>.icon.left{left:.1rem}
.fullwidth.section-6 .gallery>.icon.right{right:.1rem}
.fullwidth.section-6 .gallery .list{position:relative;margin:0 auto;width:calc(100% - 5rem);overflow:hidden;white-space:nowrap;font-size:0;text-align:left}
.fullwidth.section-6 .gallery .item{display:inline-block;width:calc(33.333% - 1.6rem);margin:.5rem .8rem;border:solid 0 #d3d3d3;font-size:1rem}
.fullwidth.section-6 .gallery .item img{width:100%;height:auto;border-radius:3px;box-shadow:0 0 4px 0 #999}
.fullwidth.section-6 .gallery .item .title{white-space:initial;margin:1rem;line-height:1.5;font-size:1.2rem;text-align:center}
.fullwidth.section-7{padding:160px 0 160px}
.fullwidth.section-1 h3,.fullwidth.section-2 h3,.fullwidth.section-3 h3,.fullwidth.section-4 h3,.fullwidth.section-5 h3,.fullwidth.section-6 h3,.fullwidth.section-7 h3{font-weight:400}
.fullwidth.section-6+.footer,.fullwidth.section-7+.footer{margin-top:0}
.fullwidth .two-columns .icon{background-image:url(images/v3/icons.png)}
.fullwidth .two-columns .icon.icon-1{background-position:-26px -26px}
.fullwidth .two-columns .icon.icon-2{background-position:-26px -126px}
.fullwidth .two-columns .icon.icon-3{background-position:-26px -226px}
.fullwidth .two-columns .icon.icon-4{background-position:-26px -326px}
.fullwidth .two-columns .icon.icon-5{background-position:-26px -426px}
.fullwidth .two-columns .icon.icon-6{background-position:-26px -526px}
.fullwidth .two-columns .icon.icon-7{background-position:-26px -626px}
.fullwidth .two-columns .icon.icon-8{background-position:-26px -726px}
.fullwidth .two-columns .icon.icon-9{background-position:-26px -826px}
.fullwidth .two-columns .icon.icon-10{background-position:-26px -926px}
.ui.orange.button,.ui.orange.buttons{height: 60px;background-color:#00a0e9;border: none;border-radius: 5px;text-align:center;width:325px;font-size: 30px;color: #fff;}
.ui.orange.button:hover,.ui.orange.buttons .button:hover{background-color:#f99848}
