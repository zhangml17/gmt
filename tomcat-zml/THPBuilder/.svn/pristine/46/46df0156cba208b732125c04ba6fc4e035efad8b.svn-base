.table>tbody>tr>td { line-height: 40px; text-align: center; vertical-align: middle;}
.item-table .first-td { min-width: 80px; }
.item-table td img {margin-right: 10px;}
.item-table .tags a { display: inline-block; height:24px; line-height: 24px; background-color: #00a0e9; color:#fff; padding:0 5px; border-radius: 3px; margin-right: 8px;  }
.item-table h5 { font-weight: bold;}

.fz-page-nav { text-align: center;}
