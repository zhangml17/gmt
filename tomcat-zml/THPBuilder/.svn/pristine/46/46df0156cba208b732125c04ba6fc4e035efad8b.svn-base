/*编辑仿真*/
.edit-box { background-color: #fff; padding:20px; }
.edit-box textarea { width:100%; max-width:100%; outline:none; padding-left: 10px; border:1px solid #ccc; border-radius: 3px; }
.edit-box input[type=radio] { width:18px; height:20px; margin-right: 4px; line-height: 25px; margin-top: 0; vertical-align: middle; }
.edit-box select { width:300px; height:45px; margin-right: 20px; line-height: 45px; outline:none; text-indent: 10px; border:1px solid #ccc; border-radius: 3px;}
.edit-box button.close { width:30px; height:30px; background: transparent; color:#666; }

/*个人主页*/
.person-bar { padding:0 20px 30px;}
.person-avatar {text-align: center; padding-top: 20px; border-radius: 95px;}
.person-avatar img { width:100%;}
.person-bar h3 { text-align: center;}
.person-bar p { line-height: 22px; border-bottom: 1px solid #ccc; padding-bottom: 5px;}
.person-bar p span { color:#747474;}
.person-bar p em { font-weight: bold; font-style: normal; color:#666;}
.person-bar p em a { color:#666; text-decoration: underline;}
.public-box .txt-box p { height: 100px; padding-right: 15px; color:#666;}
.public-box .txt-box a.author { color:#f37020; font-size: 16px; font-weight: bold; background: none; text-decoration: underline;}
.public-box .txt-box a:hover { color:#00a0e9;}
.public-box .txt-box span { color:#a1a1a1;}

/*搜索页面*/
.searching, .item-market { background-color: #fff; margin: 0px auto;padding-top:103px; }
.searching .search-box { padding:20px 20px 0; border-bottom: 1px solid #ccc;}
.searching .input-group { margin-bottom: 10px;}
.searching .form-control { height:40px;}
.searching .searching-btn { height:40px; font-size: 16px; cursor: pointer; background: #00a0e9; color:#fff; border-color:#00a0e9;}
.searching p { color:#666;}
.searching .result-box { padding:0px 20px 20px;}
.search-box-tip strong { color:#F01400; }

.result-box .public-box { padding:20px 0; border-bottom: 1px solid #ccc;}

/*仿真广场*/
