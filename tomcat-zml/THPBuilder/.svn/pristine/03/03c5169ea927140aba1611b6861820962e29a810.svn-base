<script>
    $('.fz-navbar-nav li').click(function(e) {
        $('.fz-navbar-nav li').removeClass('active');
        $(this).addClass('active');
    });
    
    $(document).ready(function() {
        //粒子背景特效
        $('#fh5co-started').particleground({
            dotColor: '#00a0e9',
            lineColor: '#00a0e9',
            minSpeedX: 0.2,
            minSpeedY: 0.2,
            directionX: 'center',
        });
        $('#try_btn').click(function() {
            if (getCookie('loginStatus')) {
                window.location.href = "/editor/";
            } else {
                off_key = 'toeditor';
                start();
            }
        });
        $('#try_btn2').click(function() {
            if (getCookie('loginStatus')) {
                window.location.href = "/editor/?editorType=1";
            } else {
                off_key = 'toeditor';
                start();
            }
        });
        $('#try_now').click(function() {
            if (getCookie('loginStatus')) {
                window.location.href = "/editor/";
            } else {
                off_key = 'toeditor';
                start();
            }
        });   
        
    });
</script>
    
